import java.util.Scanner;
class SumOfTwo
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter first number: ");
		int a=sc.nextLine();

		System.out.println("Enter second number: ");
		int b=sc.nextLine();

		int sum=a+b;
		System.out.println(sum);
	}
}
